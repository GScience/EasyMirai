<?xml version="1.0" encoding="utf-8"?>
<event
	xmlns="http://tempuri.org/Mirai.xsd"
	name="GroupRecallEvent"
	desc="群消息撤回">
	<val name="authorId" type="Long" desc="原消息发送者的QQ号"/>
	<val name="messageId" type="Int" desc="原消息messageId"/>
	<val name="time" type="Int" desc="原消息发送时间"/>
	<ref name="group" ref="Group" desc="消息撤回所在的群"/>
	<ref name="operator" ref="Member" desc="撤回消息的操作人，当null时为bot操作"/>
</event>