<?xml version="1.0" encoding="utf-8"?>
<event
	xmlns="http://tempuri.org/Mirai.xsd"
	name="BotInvitedJoinGroupRequestEvent"
	desc="Bot被邀请入群申请">
	<val name="eventId" type="Long" desc="事件标识，响应该事件时的标识"/>
	<val name="fromId" type="Long" desc="邀请人（好友）的QQ号"/>
	<val name="groupId" type="Long" desc="被邀请进入群的群号"/>
	<val name="groupName" type="String" desc="被邀请进入群的群名称"/>
	<val name="nick" type="String" desc="邀请人（好友）的昵称"/>
	<val name="message" type="String" desc="邀请消息"/>
</event>