<?xml version="1.0" encoding="utf-8"?>
<event
	xmlns="http://tempuri.org/Mirai.xsd"
	name="GroupNameChangeEvent"
	desc="某个群名改变">
	<val name="origin" type="String" desc="原群名"/>
	<val name="current" type="String" desc="新群名"/>
	<ref name="group" ref="Group" desc="群名改名的群信息"/>
	<ref name="operator" ref="Member" desc="操作的管理员或群主信息，当null时为Bot操作"/>
</event>